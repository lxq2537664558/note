CREATE ALGORITHM=UNDEFINED DEFINER=`jshd1005`@`%` 
SQL SECURITY DEFINER VIEW `user_group_level` AS select 
        `vc_member`.`userName` AS `userName`,
        `vc_member`.`levelId` AS `levelId`,
        `vc_group_level`.`groupId` AS `groupId`,
        `vc_member`.`scName` AS `scName` 
            
    from (`vc_member` join `vc_group_level` on(
            (
                (`vc_member`.`levelId` = `vc_group_level`.`levelId`) and (`vc_member`.`scId` = `vc_group_level`.`scId`)
            )
    ))





