CREATE TABLE `vc_deposits_member` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `member_name` varchar(32) NOT NULL DEFAULT '' COMMENT '会员名',
  `member_id` int UNSIGNED NOT NULL DEFAULT 0 COMMENT '会员id',
  `dep_amount` decimal(16,2) NOT NULL DEFAULT '0.00' COMMENT '入款金额',
  `wit_amount` decimal(16,2) NOT NULL DEFAULT '0.00' COMMENT '出款金额',
  `dep_total` int UNSIGNED NOT NULL DEFAULT 0 COMMENT '入款次数',
  `wit_total` int UNSIGNED NOT NULL DEFAULT 0 COMMENT '出款次数',
  `agent_name` varchar(32) NOT NULL DEFAULT '' COMMENT '代理账号',
  `time_day` int(10) unsigned NOT NULL DEFAULT '0' COMMENT '日期',
  PRIMARY KEY (`id`),
  KEY `member_name` (`member_name`),
  KEY `dep_amount` (`dep_amount`),
  KEY `wit_amount` (`wit_amount`),
  KEY `time_day` (`time_day`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4




